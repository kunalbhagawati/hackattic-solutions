# Initially autogenerated using Intellij
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 120
tab_width = 2
trim_trailing_whitespace = true

# noinspection EditorConfigOptionRedundancy
[{*.ats,*.cts,*.mts,*.ts}]
indent_size = 2
tab_width = 2

# noinspection EditorConfigOptionRedundancy
[{*.cjs,*.js}]
indent_size = 2
tab_width = 2

# noinspection EditorConfigOptionRedundancy
[{*.har,*.jsb2,*.jsb3,*.json,.babelrc,.eslintrc,.prettierrc,.stylelintrc,bowerrc,jest.config}]
indent_size = 2

# noinspection EditorConfigOptionRedundancy
[{*.yaml,*.yml,.analysis_options}]
indent_size = 2

# noinspection EditorConfigOptionRedundancy
[*.py]
indent_size = 2
